<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>TLSAdmin - @ViewBag.Title</title>
    <meta name="description" content="TLSAdmin">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <environment include="Development">
        @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/normalize.css@8.0.0/normalize.min.css">*@
        <link href="~/lib/normalize/normalize.min.css" rel="stylesheet" />

        @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css">*@
        <link href="~/lib/bootstrap-4.4.1-dist/css/bootstrap.min.css" rel="stylesheet" />

        @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css">*@
        <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

        @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/lykmapipo/themify-icons@0.1.2/css/themify-icons.css">*@
        <link href="~/lib/themify-icons-master/css/themify-icons.css" rel="stylesheet" />

        @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/pixeden-stroke-7-icon@1.2.3/pe-icon-7-stroke/dist/pe-icon-7-stroke.min.css">*@
        <link href="~/lib/package/pe-icon-7-stroke/dist/pe-icon-7-stroke.min.css" rel="stylesheet" />

        @*<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/flag-icon-css/3.2.0/css/flag-icon.min.css">*@
        <link href="~/lib/flag-icon-css/css/flag-icon.min.css" rel="stylesheet" />

        <link href="~/css/cs-skin-elastic.css" rel="stylesheet" />
        <link href="~/css/style.css" rel="stylesheet" />
    </environment>
    
    <environment exclude="Development">
        @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/normalize.css@8.0.0/normalize.min.css">*@
        <link href="~/lib/normalize/normalize.min.css" rel="stylesheet" />

        @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css">*@
        <link href="~/lib/bootstrap-4.4.1-dist/css/bootstrap.min.css" rel="stylesheet" />

        @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css">*@
        <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

        @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/lykmapipo/themify-icons@0.1.2/css/themify-icons.css">*@
        <link href="~/lib/themify-icons-master/css/themify-icons.css" rel="stylesheet" />

        @*<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/pixeden-stroke-7-icon@1.2.3/pe-icon-7-stroke/dist/pe-icon-7-stroke.min.css">*@
        <link href="~/lib/package/pe-icon-7-stroke/dist/pe-icon-7-stroke.min.css" rel="stylesheet" />

        @*<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/flag-icon-css/3.2.0/css/flag-icon.min.css">*@
        <link href="~/lib/flag-icon-css/css/flag-icon.min.css" rel="stylesheet" />

        <link href="~/css/cs-skin-elastic.css" rel="stylesheet" />
        <link href="~/css/style.css" rel="stylesheet" />
    </environment>

    <style>
        #weatherWidget .currentDesc {
            color: #ffffff !important;
        }

        .traffic-chart {
            min-height: 335px;
        }

        #flotPie1 {
            height: 150px;
        }

            #flotPie1 td {
                padding: 3px;
            }

            #flotPie1 table {
                top: 20px !important;
                right: -10px !important;
            }

        .chart-container {
            display: table;
            min-width: 270px;
            text-align: left;
            padding-top: 10px;
            padding-bottom: 10px;
        }

        #flotLine5 {
            height: 105px;
        }

        #flotBarChart {
            height: 150px;
        }

        #cellPaiChart {
            height: 160px;
        }
    </style>

</head>
<body>
    <!-- Begin Left Panel -->
    <aside id="left-panel" class="left-panel">
        <nav class="navbar navbar-expand-sm navbar-default">
            <div id="main-menu" class="main-menu collapse navbar-collapse">
                <ul class="nav navbar-nav">

                    <li class="menu-title">Store</li><!-- Menu Title -->
                    <!-- Begin Livros -->
                    <li class="menu-item-has-children dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class="menu-icon fa fa-book"></i>Livros</a>
                        <ul class="sub-menu children dropdown-menu">
                            <li><i class="fa fa-search"></i><a href="ui-buttons.html">Pesquisar</a></li>
                            <li><i class="fa fa-plus-square"></i><a href="ui-badges.html">Cadastrar Novo</a></li>
                            <li><i class="fa fa-edit"></i><a href="ui-tabs.html">Gerenciar</a></li>
                        </ul>
                    </li>
                    <!-- End Livros -->
                    <!-- Begin Autores -->
                    <li class="menu-item-has-children dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class="menu-icon fa fa-user"></i>Autores</a>
                        <ul class="sub-menu children dropdown-menu">
                            <li><i class="fa fa-search"></i><a href="ui-buttons.html">Pesquisar</a></li>
                            <li><i class="fa fa-plus-square"></i><a href="ui-badges.html">Cadastrar Novo</a></li>
                            <li><i class="fa fa-edit"></i><a href="ui-tabs.html">Gerenciar</a></li>
                        </ul>
                    </li>
                    <!-- End Autores -->

                    <li class="menu-title">Admin</li><!-- /.menu-title -->
                    <!-- Begin Usuários -->
                    <li class="menu-item-has-children dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class="menu-icon fa fa-users"></i>Usuários</a>
                        <ul class="sub-menu children dropdown-menu">
                            <li><i class="fa fa-search"></i><a href="ui-buttons.html">Pesquisar</a></li>
                            <li><i class="fa fa-plus-square"></i><a href="ui-badges.html">Cadastrar Novo</a></li>
                            <li><i class="fa fa-edit"></i><a href="ui-tabs.html">Gerenciar</a></li>
                        </ul>
                    </li>
                    <!-- End Usuários -->
                    <!-- Begin Perfis -->
                    <li class="menu-item-has-children dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class="menu-icon fa fa-id-card"></i>Perfis</a>
                        <ul class="sub-menu children dropdown-menu">
                            <li><i class="fa fa-search"></i><a href="ui-buttons.html">Pesquisar</a></li>
                            <li><i class="fa fa-plus-square"></i><a href="ui-badges.html">Cadastrar Novo</a></li>
                            <li><i class="fa fa-edit"></i><a href="ui-tabs.html">Gerenciar</a></li>
                        </ul>
                    </li>
                    <!-- End Perfis -->

                </ul>
            </div><!-- /.navbar-collapse -->
        </nav>
    </aside>
    <!-- End Left Painel -->
    <!-- Begin Right Panel -->
    <div id="right-panel" class="right-panel">
        <!-- Header-->
        <header id="header" class="header">
            <div class="top-left">
                <div class="navbar-header">
                    <a class="navbar-brand" asp-controller="Home" asp-action="Index">TLSAdmin</a>
                    <a class="navbar-brand hidden" href="./"><img src="images/logo2.png" alt="Logo"></a>
                    <a id="menuToggle" class="menutoggle"><i class="fa fa-bars"></i></a>
                </div>
            </div>
            <div class="top-right">
                <div class="header-menu">
                    <div class="user-area dropdown float-right">
                        <a href="#" class="dropdown-toggle active" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <img class="user-avatar rounded-circle" src="~/images/profile.png" alt="User Avatar">
                        </a>

                        <div class="user-menu dropdown-menu">
                            <a class="nav-link" href="#"><i class="fa fa- user"></i>My Profile</a>
                            <a class="nav-link" asp-controller="Authentication" asp-action="Logout"><i class="fa fa-power -off"></i>Logout</a>
                        </div>
                    </div>

                </div>
            </div>
        </header>
        <!-- /#header -->
    </div>

    <partial name="_CookieConsentPartial" />

    <div class="container body-content">
        @RenderBody()
        <hr />
    </div>


    <environment include="Development">
        <!-- Scripts -->
        @*<script src="https://cdn.jsdelivr.net/npm/jquery@2.2.4/dist/jquery.min.js"></script>*@
        <script src="~/lib/jquery/jquery.min.js"></script>

        @*<script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.4/dist/umd/popper.min.js"></script>*@
        <script src="~/lib/popper.js/umd/popper.min.js"></script>

        @*<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js"></script>*@
        <script src="~/lib/bootstrap-4.4.1-dist/js/bootstrap.min.js"></script>

        @*<script src="https://cdn.jsdelivr.net/npm/jquery-match-height@0.7.2/dist/jquery.matchHeight.min.js"></script>*@
        <script src="~/lib/jquery.matchHeight/jquery.matchHeight-min.js"></script>

        <script src="~/js/main.js"></script>
    </environment>

    <environment exclude="Development">
        <!-- Scripts -->
        @*<script src="https://cdn.jsdelivr.net/npm/jquery@2.2.4/dist/jquery.min.js"></script>*@
        <script src="~/lib/jquery/jquery.min.js"></script>

        @*<script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.4/dist/umd/popper.min.js"></script>*@
        <script src="~/lib/popper.js/umd/popper.min.js"></script>

        @*<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js"></script>*@
        <script src="~/lib/bootstrap-4.4.1-dist/js/bootstrap.min.js"></script>

        @*<script src="https://cdn.jsdelivr.net/npm/jquery-match-height@0.7.2/dist/jquery.matchHeight.min.js"></script>*@
        <script src="~/lib/jquery.matchHeight/jquery.matchHeight-min.js"></script>

        <script src="~/js/main.js"></script>
    </environment>
    


    @RenderSection("Scripts", required: false)
</body>
</html>
